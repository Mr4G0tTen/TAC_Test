    public static BigDecimal cleanToDecimal(String input, String columnName) {
        if (input == null || input.trim().isEmpty()) {
            return BigDecimal.ZERO;
        }

        boolean isNegative = input.contains("(") && input.contains(")");

        // Bersihkan semua karakter kecuali angka, koma, titik, minus
        String cleaned = input.replaceAll("[^0-9,.-]", "");

        // Deteksi format
        boolean isLocalized = input.contains(",") && !input.contains("."); // format ID (pakai koma)
        //boolean isDecimalWithDot = input.contains(".") && !input.contains(","); // format EN

        // Kalau format lokal, buang titik (ribuan), ubah koma jadi titik
        if (isLocalized) {
            cleaned = cleaned.replace(".", ""); // buang titik ribuan
            cleaned = cleaned.replace(",", "."); // ubah koma jadi titik desimal
        }

        // Kalau campuran (punya koma & titik), anggap format salah → buang semua titik
        if (input.contains(",") && input.contains(".")) {
            cleaned = cleaned.replace(".", "");
            cleaned = cleaned.replace(",", ".");
        }

        // Jika masih lebih dari 1 titik, log warning dan buang semua titik
        int dotCount = cleaned.length() - cleaned.replace(".", "").length();
        if (dotCount > 1) {
            //System.err.println("WARNING: Kolom '" + columnName + "' mengandung >1 titik: " + input + " → cleaned: " + cleaned);
            cleaned = cleaned.replace(".", "");
        }

        if (!cleaned.matches(".*[0-9].*")) {
            return BigDecimal.ZERO;
        }

        if (isNegative) {
            cleaned = "-" + cleaned.replace("(", "").replace(")", "");
        }

        try {
            return new BigDecimal(cleaned);
        } catch (Exception e) {
            throw new NumberFormatException("Gagal convert kolom '" + columnName + "' dengan nilai asli: \"" + input + "\", hasil cleaning: \"" + cleaned + "\"");
        }
    }

