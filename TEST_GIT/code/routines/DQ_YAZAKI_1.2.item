    // Method untuk memindahkan "PT", "CV", atau "UD" ke bagian depan
    private static String moveCompanyToFront(String address) {
        // Hapus koma di akhir kalimat jika ada
        address = address.replaceAll(",$", "").trim();

        // Mencari "PT", "CV", atau "UD" dengan tanda titik dan spasi tidak perlu (case-insensitive)
        String regex = "(?i)\\b(PT|CV|UD)\\s*\\.\\s*";
        address = address.replaceAll(regex, "$1 "); // Mengganti "PT .", "CV .", atau "UD ." menjadi "PT", "CV", atau "UD"

        // Mencari kembali "PT", "CV", atau "UD" di string
        regex = "(?i)\\b(PT|CV|UD)\\b";
        java.util.regex.Pattern pattern = java.util.regex.Pattern.compile(regex);
        java.util.regex.Matcher matcher = pattern.matcher(address);

        // Jika ditemukan "PT", "CV", atau "UD", pindahkan ke depan
        if (matcher.find()) {
            String legalEntity = matcher.group(1).toUpperCase(); // Mengubah menjadi uppercase (PT, CV, atau UD)
            address = matcher.replaceFirst("").trim(); // Hapus "PT", "CV", atau "UD" dari posisi awalnya
            return legalEntity + " " + address; // Menggabungkan "PT", "CV", atau "UD" di depan
        }
        return address; // Mengembalikan nama asli jika tidak ada "PT", "CV", atau "UD"
    }
	
	public static String fixTelephone(String supplier_telephone) {
        if (supplier_telephone == null || supplier_telephone.isEmpty()) {
            return supplier_telephone;
        }

        // Menghapus tanda kurung (), tanda -, spasi, dan tanda []
        String fixedTelephone = supplier_telephone.replaceAll("[()\\-\\s\\[\\]\\'\\+]", "");


        // Jika nomor telepon diawali dengan 08 atau 8, ubah awalan menjadi 628
        if (fixedTelephone.startsWith("08") && !fixedTelephone.startsWith("0800")) {
            fixedTelephone = "628" + fixedTelephone.substring(2); // Ubah 08 menjadi 628
        } else if (fixedTelephone.startsWith("8") && !fixedTelephone.startsWith("800") && !fixedTelephone.startsWith("86")
        	&& !fixedTelephone.startsWith("8868") && !fixedTelephone.startsWith("84") && !fixedTelephone.startsWith("81/") && !fixedTelephone.startsWith("8522302"))  {
            fixedTelephone = "628" + fixedTelephone.substring(1); // Ubah 8 menjadi 628
        } else if (fixedTelephone.startsWith("6221")) {
            fixedTelephone = "021" + fixedTelephone.substring(4); // Ubah 6221 menjadi 021
        } else if (fixedTelephone.matches("^81/\\d+$")) {
            fixedTelephone = fixedTelephone.replace("/", ""); // Hapus tanda garis miring '/' untuk menggabungkan nomor
        }
        
        // Menambahkan spasi antara angka dan huruf (misalnya: 7155463070Ext101 -> 7155463070 Ext 101)
        fixedTelephone = fixedTelephone.replaceAll("(?<=\\d)(?=[a-zA-Z])|(?<=[a-zA-Z])(?=\\d)", " ");

        // Jika ada tanda slash "/", pisahkan menjadi dua nomor telepon
        if (fixedTelephone.contains("/")) {
            String[] parts = fixedTelephone.split("/");

            if (parts.length == 2) {
                String baseNumber = parts[0].trim();
                String extension = parts[1].trim();

                // Jika extension adalah nomor telepon lengkap (misalnya 087781980953)
                if (extension.startsWith("08")) {
                    extension = "628" + extension.substring(2); // Ubah 08 menjadi 628
                } else if (extension.length() == 2) {
                    // Jika extension hanya dua digit (misalnya '48'), gabungkan dengan baseNumber
                    extension = baseNumber.substring(0, baseNumber.length() - 2) + extension;
                }

                // Tidak perlu menambahkan kode area dari baseNumber ke extension jika nomor lengkap
                fixedTelephone = baseNumber + ", " + extension;
            }
        }

        // Jika ada koma, tambahkan kembali awalan dari nomor pertama ke nomor kedua
        if (fixedTelephone.contains(",")) {
            String[] parts = fixedTelephone.split(",");
            String baseNumber = parts[0].trim();
            String secondNumber = parts[1].trim();

            // Periksa apakah secondNumber dimulai dengan angka
            if (secondNumber.matches("^[0-9].*")) {
                // Jika secondNumber diawali dengan 08, ubah menjadi 628
                if (secondNumber.startsWith("08")) {
                    secondNumber = "628" + secondNumber.substring(2); // Ubah 08 menjadi 628
                }
                // Jika secondNumber tidak memiliki kode area yang lengkap, tambahkan kode area dari baseNumber
                else if (!secondNumber.startsWith("62") && !secondNumber.startsWith(baseNumber.substring(0, 3))) {
                    String areaCode = baseNumber.substring(0, 3); // Ambil kode area (3 digit pertama dari baseNumber)
                    secondNumber = areaCode + secondNumber; // Tambahkan kode area ke secondNumber
                }
            } else {
                // Jika secondNumber bukan angka, hanya tambahkan spasi jika diperlukan
                secondNumber = secondNumber.replaceAll("(?<=\\D)(?=\\d)", " ");
            }

            fixedTelephone = baseNumber + ", " + secondNumber;
        }
        
        // Tambahan: Jika ada beberapa nomor telepon yang dipisahkan oleh tanda slash (misalnya: 0318068475/0315480955/081803296690)
        // Ubah format menjadi nomor yang dipisahkan oleh koma (misalnya: 0318068475, 0315480955, 081803296690)
        if (fixedTelephone.contains("/") && !fixedTelephone.matches("81/\\d+")) {
            String[] parts = fixedTelephone.split("/");

            StringBuilder formattedNumbers = new StringBuilder();
            for (int i = 0; i < parts.length; i++) {
                String part = parts[i].trim();

                // Jika ada nomor yang dimulai dengan 08, ubah ke format 628
                if (part.startsWith("08")) {
                    part = "628" + part.substring(2); // Ubah 08 menjadi 628
                }

                if (i > 0) {
                    formattedNumbers.append(", ");
                }
                formattedNumbers.append(part);
            }

            fixedTelephone = formattedNumbers.toString();
        }

        return fixedTelephone;
    }
	
