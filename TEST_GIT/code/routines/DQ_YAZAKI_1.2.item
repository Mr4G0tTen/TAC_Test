 // Daftar scope yang akan digunakan sebagai pemisah
    private static final List<String> SCOPES = Arrays.asList(
        "FINISH_GOOD", "RAW_MATERIAL", "PRICING", "SUPPLIER", "INVENTORY", "FINANCE", "DOWNTIME_PROD"
    );

    // Method untuk satu parameter
    public static String getTableName(String source) {
        if (source == null || source.isEmpty()) {
            return "";
        }

        // Loop melalui daftar scope untuk mencari yang ada dalam string
        for (String scope : SCOPES) {
            int index = source.indexOf(scope);
            if (index != -1) {
                // Ambil substring setelah kata scope
                int startIndex = index + scope.length() + 1; // +1 untuk melewati underscore (_)
                if (startIndex < source.length()) {
                    return source.substring(startIndex);
                }
            }
        }
        return source; // Jika tidak ditemukan, kembalikan string asli
    }

    // Method untuk menerima satu atau lebih parameter (varargs)
    public static String getTableNames(String... sources) {
        if (sources == null || sources.length == 0) {
            return "";
        }

        // Proses semua input dan ambil hasilnya
        StringBuilder result = new StringBuilder();
        for (String source : sources) {
            if (source != null && !source.isEmpty()) {
                if (result.length() > 0) {
                    result.append(", "); // Pisahkan dengan koma jika ada lebih dari satu nilai
                }
                result.append(getTableName(source));
            }
        }
        return result.toString();
    }
    
