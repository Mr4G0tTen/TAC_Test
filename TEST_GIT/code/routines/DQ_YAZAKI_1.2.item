	// validate address name
	public static String fixAddress(String ADDRESS_1_1) {
        if (ADDRESS_1_1 == null) {
            return null; // Mengembalikan null jika input null
        }

        // Mengganti tanda titik (.), koma (,) dengan format ".space" atau ",space"
        ADDRESS_1_1 = ADDRESS_1_1.replaceAll("\\s*([.,])\\s*", "$1 ");

        // Mengganti tanda "-", "/" dengan " spasi-spasi, spasi/spasi"
        ADDRESS_1_1 = ADDRESS_1_1.replaceAll("\\s*([-/])\\s*", " $1 ");

        // Menggunakan regex untuk mencocokkan berbagai varian kata "jalan" menjadi "JL."
        ADDRESS_1_1 = ADDRESS_1_1.replaceAll("(?i)\\b(jalan|jl|jln|jlm)\\b\\.?", "JL.");
        
        // Menggunakan regex untuk mencocokkan kata "kp" menjadi "KP."
        ADDRESS_1_1 = ADDRESS_1_1.replaceAll("(?i)\\b(kp)\\b\\.?", "KP.");

        // Mengganti varian dari kata "komplek", "komp.", "kom", dll. menjadi "KOMPLEKS"
        ADDRESS_1_1 = replaceKompleks(ADDRESS_1_1);

        // Mengganti kata "KAV" atau variannya menjadi "KAVLING"
        ADDRESS_1_1 = replaceKav(ADDRESS_1_1);
		
		// Mengganti kata "KAW" atau variannya menjadi "KAWASAN"
        ADDRESS_1_1 = replaceKaw(ADDRESS_1_1);
		
		// Mengganti kata "GED, GD" atau variannya menjadi "GEDUNG"
        ADDRESS_1_1 = replaceGed(ADDRESS_1_1);
		
		// Mengganti kata "GG" atau variannya menjadi "GANG"
        ADDRESS_1_1 = replaceGang(ADDRESS_1_1);
		
		// Mengganti kata "DSN" atau variannya menjadi "DUSUN"
        ADDRESS_1_1 = replaceDsn(ADDRESS_1_1);
		
		// Mengganti kata "DS" atau variannya menjadi "DESA"
        ADDRESS_1_1 = replaceDs(ADDRESS_1_1);
		
		// Menambahkan logika untuk alamat khusus "1/89 MOO 5, ROJANA INDUSTRIAL PARK"
        ADDRESS_1_1 = fixSpecialAddress(ADDRESS_1_1);

        // Pindahkan "PT", "CV", atau "UD" ke depan jika ada, dan hapus spasi yang tidak perlu
        ADDRESS_1_1 = moveCompanyToFront(ADDRESS_1_1);
        
        // ubah address affiliates menggunakan alamat affiliates - RFD 3 Januari 2024
        ADDRESS_1_1 = fixAffiliatesToAddress(ADDRESS_1_1);
        
        ADDRESS_1_1 = fixAddressName(ADDRESS_1_1);
        
        ADDRESS_1_1 = FixEmptyHash(ADDRESS_1_1);

        return ADDRESS_1_1.trim(); // Mengembalikan hasil akhir dengan trim untuk memastikan tidak ada spasi berlebih
    }

    // Method tambahan untuk mengganti varian "komplek", "komp.", dll. menjadi "KOMPLEKS"
    private static String replaceKompleks(String address) {
        // Regex untuk mencocokkan berbagai varian "komplek", "komp.", "kom", "kmplk"
        String regex = "(?i)\\b(kompleks|komplex|komplek|komp|kom|kmplk)\\b\\.?";
        // Mengganti dengan "KOMPLEKS" dan menghapus tanda titik
        return address.replaceAll(regex, "KOMPLEKS");
    }

    // Method tambahan untuk mengganti varian "KAV" menjadi "KAWASAN"
    private static String replaceKav(String address) {
