    // utk fix address name
    private static String fixAddressName(String address) {
        // Regex untuk setiap pola
        String regexGS = "\\b(GTOT|ATOT|G|RATOT)\\b(?=\\s+SUBROTO)";
        String regexTMR = "\\b(TMR)\\b";
        String regexRTRW = "\\b(RT / RW ; 07 / 07)\\b";

        // Proses setiap pola satu per satu
        if (address.matches(".*" + regexGS + ".*")) {
            // Ganti bagian yang cocok dengan "GATOT"
            address = address.replaceAll(regexGS, "GATOT");
        }
        if (address.matches(".*" + regexTMR + ".*")) {
            // Ganti "TMR" dengan "TIMUR"
            address = address.replaceAll(regexTMR, "TIMUR");
        }
        if (address.matches(".*" + regexRTRW + ".*")) {
            // Ganti format menjadi "RT/RW:07/07"
            address = address.replaceAll(regexRTRW, "RT/RW:07/07");
        }

        // Kembalikan hasil akhir
        return address;
    }
    
