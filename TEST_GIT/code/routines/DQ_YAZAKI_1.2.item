	//Improve Completness
	public static String Completeness(String columnName, String value) {
		    if (value == null || value.trim().isEmpty() || value.trim().equalsIgnoreCase("null")) {
		        return columnName + " must not be null";
		    }
		    return ""; // Return null if the value is valid
		}
		
		//Check Numeric Format
	public static String validateNumericFormat(String columnName, String value) {
	        if (!value.matches("^[0-9\\,\\.\\-]+$")) {
	            return columnName + " data not numeric format";
	        }
	        
	        return "";
	    }
		
	//Check Integrity Date
	public static String validityDate(String columnName, Date value) { 
		int year = Integer.parseInt(TalendDate.formatDate("yyyy", value));
		//int currentYear = Integer.parseInt(TalendDate.formatDate("yyyy", TalendDate.getCurrentDate()));

		    if (year < 2000) {
		        return columnName + " data validity violation";
		    }
		    
		    return "";
		}
					
	// Remove Number
	public static String removeNumber(String param) {
	    return param != null ? param.replaceAll("\\d", "") : param;
	}
	
	//Validation HS Code
	public static String validateRemarkHSCode(String remarkHSCode, String remarkSite) {
        List<String> validations = new ArrayList<>();

        // Parse HS_CODE dan cek konsistensi
        Set<String> hsCodes = new HashSet<>();
        if (remarkHSCode != null && !remarkHSCode.trim().isEmpty()) {
            String[] hsParts = remarkHSCode.split("\\s*,\\s*");
            hsCodes.addAll(Arrays.asList(hsParts));
            if (hsCodes.size() > 1) {
                validations.add("Not Consistence HS_CODE");
            } else if (hasDuplicates(hsParts)) {
                validations.add("Not Unique HS_CODE");
            }
        }

        // Parse SITE dan cek kondisi SAMI_JF / SAMI_TF
        if (remarkSite != null && !remarkSite.trim().isEmpty()) {
            String[] sites = remarkSite.split("\\s*,\\s*");
            Set<String> siteSet = new HashSet<>(Arrays.asList(sites));

            if (siteSet.contains("SAMI_TF") || siteSet.contains("SAMI_JF")) {
                validations.add("SAMITF/SAMIJF Not Consistency");
            }

            if (siteSet.size() == 1 && siteSet.contains("SUAI")) {
                if (hsCodes.size() > 1) {
                    validations.add("SUAI Not Consistence");
                }
            }

            if (hasDuplicates(sites)) {
                validations.add("Not Unique Site");
            }
        }

        return String.join(", ", validations);
    }

    private static boolean hasDuplicates(String[] array) {
        Set<String> seen = new HashSet<>();
        for (String item : array) {
            if (!seen.add(item)) {
                return true;
            }
        }
        return false;
    }
	
	// Remove Special Character
	public static String removeSpecialCharacter(String param) {
	    return param != null ? param.replaceAll("[^A-Za-z0-9]", "").trim() : param;
	}

	// Remove TandaBaca
	public static String removePunctuation(String param) {
	    return param != null ? param.replaceAll("\\p{Punct}", " ") : param;
	}

	// Merubah Data Null Menjadi NA
	public static String changeNull(String param) {
	    if (param == null || param.trim().isEmpty() || param.trim().equalsIgnoreCase("NULL")) {
	        return "NA";
	    } else {
	        return param;
	    }
	}
	
	public static String emptyEnter(String param) {
		// Menghapus newline
        String cleanedData = param
                .replaceAll("\\s+", " ")
                .trim();
        
        return cleanedData.trim();
	}
	
	public static String getLongestSubstring(String input, String delimiter) {
        if (input == null || input.isEmpty()) {
            return "";
        }
        
        String[] parts = input.split(delimiter);
        String longest = "";
        
        for (String part : parts) {
            if (part.trim().length() > longest.length()) {
                longest = part.trim();
            }
        }
        
        return longest;
	}

