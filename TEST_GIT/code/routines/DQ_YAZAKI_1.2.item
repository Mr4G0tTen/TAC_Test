    //ubah menggunakan legalName
    public static String IDUltimateSupplier(String OriCC) {
        if (OriCC == null || OriCC.equalsIgnoreCase("NOT AVAILABLE")) {
            return null;
        }

        String upper = OriCC.toUpperCase();

        if (upper.contains("YAZAKI") || upper.contains("AUTOCOMP")) {
            return "Z004";
        }

        return "Z003";
    }
 
    public static String removeNickName(String name) {
        if (name == null) return "";

        String nameParam = name.toUpperCase();

        // Khusus nama tertentu
        if (nameParam.contains("H E R Y")) return "HERY";
        if (nameParam.contains("VIDYAD@MINDASEAN.COM")) return "VIDYAD";
        if (nameParam.contains("H.ITO")) return "ITO";

        // Normalisasi slash
        nameParam = nameParam.replaceAll("\\s*/\\s*", " / ");

        // Hapus "H." atau "H " hanya jika di depan nama
        nameParam = nameParam.replaceFirst("^H\\.?\\s+", "");

        // Hapus gelar/prefix lain di mana pun (kecuali H.)
        nameParam = nameParam.replaceAll("\\b(BPK\\.?|BP\\.?|MR\\.?|MSC\\.?|DRS\\.?|IBU\\.?|IR\\.?|BAPAK)\\b", "");

        // Hapus sisa titik awal jika ada
        nameParam = nameParam.replaceAll("^\\s*\\.+\\s*", "");

        // Hapus gelar akademik/sertifikasi di belakang nama
        nameParam = nameParam.replaceAll("(,?\\s*\\b(DR|DRS|SH|MH|SE|S\\.T|S\\.KOM|S\\.SI|M\\.T|M\\.KOM|PH\\.D|MBA|CLA|,SKOM)\\.?)+\\s*$", "");

        // Rapikan spasi
        nameParam = nameParam.replaceAll("\\s{2,}", " ").trim();
        nameParam = nameParam.replaceAll("\\s*/\\s*", " / ");

        return nameParam.trim();
    }
    
